<h1>Вопросы теста: <%= @test.title %></h1>

<ul>
  <% @questions.each do |question| %>
    <li><%= link_to question.body, question_path(question) %></li>
  <% end %>
</ul>
<p><%= button_to 'Создать', new_test_question_path(@test), method: :get %></p>

<p><%= button_to 'Назад', tests_path, method: :get %></p>
