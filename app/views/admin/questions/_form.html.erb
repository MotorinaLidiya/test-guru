<%= form_with model: [:admin, @test, @question], local: true do |form| %>
  <%= render 'shared/errors', resource: @question %>

  <p>
    <%= form.label :body %>
    <%= form.text_field :body, id: 'question-body' %>
  </p>

  <div>
    <%= form.submit t('helpers.submit.questions.save') %>
  </div>
<% end %>
